<%- include( '../templates/headInclude',{absPath:"pages/games/index"}) %>
<body class="container-fluid " >
    <main class="text-center"  data-aos="fade-in">
        <h1>-Games-</h1>
    <div class="container pad5">
      
      <div id="carousel" class="carousel slide col-sm " data-bs-ride="carousel" interval="2000" mx-300>
        <ol class="carousel-indicators">
          <li data-bs-target="#carousel" data-bs-slide-to="0" class="active"></li>
          <li data-bs-target="#carousel" data-bs-slide-to="1"></li>
          <li data-bs-target="#carousel" data-bs-slide-to="2"></li>
        </ol>
        <div class="carousel-inner">
          <div class="carousel-item active">
            <a href="/games/game_magurossy/"><img src="../img/magurossyRoad.png"  class="d-block w-100" alt="maguro"></a>
          </div>
          <div class="carousel-item">
            <a href="/games/game_jetSaber/"><img src="../img/JetSaber.png"  class="d-block w-100" alt="jetsaber"></a>
          </div>
          <div class="carousel-item">
            <a href="/games/game_find/"><img src="../img/Find.png"  class="d-block w-100" alt="find"> </a>
          </div>
        </div>
        <a class="carousel-control-prev" href="#carousel" role="button" data-bs-slide="prev">
          <span class="carousel-control-prev-icon" aria-hidden="true"></span>
          <span class="visually-hidden">Previous</span>
        </a>
        <a class="carousel-control-next" href="#carousel" role="button" data-bs-slide="next">
          <span class="carousel-control-next-icon" aria-hidden="true"></span>
          <span class="visually-hidden">Next</span>
        </a>
      </div>
    </div>
    
</main>
  </body>